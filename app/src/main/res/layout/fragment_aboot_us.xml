<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AbootUs">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:text="@string/about_us_text"
        android:background="@drawable/text_bg2"
        android:fontFamily="@font/font2"
        android:textColor="@color/textColor"
        android:textSize="27sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="250dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="351dp"
        android:background="@drawable/team_photo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:background="@drawable/text_bg2"
        android:fontFamily="@font/font2"
        android:text="@string/about_us_array"
        android:textColor="@color/textColor"
        android:textSize="27sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="384dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/about_us_text0"
        android:background="@drawable/text_bg2"
        android:fontFamily="@font/font2"
        android:textColor="@color/textColor"
        android:textSize="27sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="438dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/about_us_text1"
        android:background="@drawable/text_bg2"
        android:fontFamily="@font/font2"
        android:textColor="@color/textColor"
        android:textSize="27sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="476dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/about_us_text2"
        android:background="@drawable/text_bg2"
        android:fontFamily="@font/font2"
        android:textColor="@color/textColor"
        android:textSize="27sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="517dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="@string/about_us_text3"
        android:background="@drawable/text_bg2"
        android:fontFamily="@font/font2"
        android:textColor="@color/textColor"
        android:textSize="27sp"
        tools:layout_editor_absoluteY="574dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="@string/about_us_text4"
        android:background="@drawable/text_bg2"
        android:fontFamily="@font/font2"
        android:textColor="@color/textColor"
        android:textSize="27sp"
        tools:layout_editor_absoluteY="606dp" />

</android.support.constraint.ConstraintLayout>